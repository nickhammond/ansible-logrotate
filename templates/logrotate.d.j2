# {{ ansible_managed }}

"{{ item.path }}" {
  {% if item.options is defined -%}
  {% for option in item.options -%}
  {{ option }}
  {% endfor -%}
  {% endif %}
  {%- if item.scripts is defined -%}
  {%- for name, scripts in item.scripts.iteritems() -%}
  {{ name }}
  {% for script in scripts %}
    {{ script }}
  {% endfor -%}
  endscript
  {% endfor -%}
  {% endif -%}
}
